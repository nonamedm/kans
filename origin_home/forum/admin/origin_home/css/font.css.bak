#nav {
height:36px; position:relative; font-family:arial, verdana, sans-serif; font-size:14px;z-index:500;
}
#nav .select {
margin:0; padding:0; list-style:none; white-space:nowrap; 
}
#nav li {
float:left; background:url(../image_s/blank_over_p.jpg); width:140px; height:35px;
}
#nav .select a {
display:block; height:36px; float:center; background: url(../image_s/blank_p.jpg); padding:0 0 0 15px; text-decoration:none; line-height:35px; white-space:nowrap; 
}
#nav .select a b {
display:block; padding:0 30px 10px 15px; background:url(../image_s/blank_p.jpg) right top;
}
#nav .select li:hover a {
background: url(../image_s/blank_over_p.gif); padding:0 0 0 15px; line-height:35px; cursor:pointer;
}
#nav .select li:hover a b {
display:block; padding:0 30px 9px 15px; background:url(../image_s/blank_over_p.jpg) right top; cursor:pointer;
}
#nav .sub {
display:none; margin:0; padding:0 0 0 60px;list-style:none;
}
#nav .sub li {background:transparent;}
#nav .select li:hover .sub {
height:20px; display:block; position:absolute; float:left; width:750px; top:37px; left:0; text-align:center;
}
#nav .select li:hover .sub li a {
display:block;height:20px; line-height:16px; float:left; background:transparent url(transparent.gif); padding:0 16px; margin:0; white-space:nowrap; border:0px solid #000; border-color:#888 #777 #666 #ddd; color:#888;font-size:11px;
}
#nav .select li:hover .sub li a:hover {
color:#000; background:#e8e8e8; border-color: #fff #333 #000 #fff; line-height:15px;
}
